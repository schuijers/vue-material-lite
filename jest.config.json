{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/components/**/*.js",
    "src/directives/**/*.js",
    "!src/**/index.js"
  ],
  "coverageDirectory": "<rootDir>/coverage",
  "coverageReporters": [
    "lcov",
    "text"
  ],
  "moduleFileExtensions": [
    "js"
  ],
  "rootDir": ".",
  "testRegex": "<rootDir>/test/.*.spec.js"
}